{
	"name": "@valerris/webpack-config",
	"version": "0.1.0",
	"main": "webpack.config.js",
	"description": "Valerris webpack configuration",
	"author": "Valerris",
	"license": "",
	"bin": {
		"vlrrs-webpack-compile": "bin/compile.js"
	},
	"private": true,
	"scripts": {
		"build:dev": "cross-env NODE_ENV=development node bin/compile.js",
		"build:prod": "cross-env NODE_ENV=production bin/compile.js",
		"build:server": "cross-env NODE_ENV=production webpack --config config/server/webpack.config.js",
		"build": "yarn build:prod && yarn build:server",
		"serve:dev": "webpack serve",
		"preserve:prod": "yarn build:prod",
		"serve:prod": "serve build",
		"clean": "rimraf node_modules yarn.lock package-lock.json"
	},
	"dependencies": {
		"@babel/core": "^7.14.6",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-env": "^7.14.5",
		"@babel/preset-react": "^7.14.5",
		"@gfx/zopfli": "^1.0.15",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
		"babel-loader": "^8.2.2",
		"babel-plugin-styled-components": "^1.13.2",
		"babel-plugin-transform-imports": "^2.0.0",
		"circular-dependency-plugin": "^5.2.2",
		"compression-webpack-plugin": "^8.0.0",
		"core-js": "^3.15.1",
		"css-loader": "^5.2.6",
		"css-minimizer-webpack-plugin": "^3.0.1",
		"html-webpack-plugin": "^5.3.1",
		"image-webpack-loader": "^7.0.1",
		"mini-css-extract-plugin": "^1.6.0",
		"null-loader": "^4.0.1",
		"postcss": "^8.3.5",
		"postcss-loader": "^6.1.0",
		"postcss-preset-env": "^6.7.0",
		"purgecss-webpack-plugin": "^4.0.3",
		"react-refresh": "^0.10.0",
		"regenerator-runtime": "^0.13.7",
		"style-loader": "^2.0.0",
		"time-fix-plugin": "^2.0.7",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0",
		"webpack-stats-plugin": "^1.0.3"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"rimraf": "^3.0.2",
		"serve": "^12.0.0",
		"styled-components": "^5.3.0"
